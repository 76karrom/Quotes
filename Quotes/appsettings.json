{
  "FeatureFlags": {
    "UseAzureStorage": true,
    "UseMongoDb": false,
    "UseCosmosDb": true
  },
  "AzureBlob": {
    "ContainerUrl": "https://{accountname}.blob.core.windows.net/{container}"
  },
  "MongoDb": {
    "SubscribersCollectionName": "Subscribers"
  },
  "CosmosDb": {
    "SubscribersCollectionName": "Subscribers"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
